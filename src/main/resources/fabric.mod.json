{
  "schemaVersion": 1,
  "id": "biome-fog",
  "version": "${version}",
  "name": "Biome Fog",
  "description": "Adds unique fog to a bunch of biomes!",
  "authors": [
    "Steveplays28"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/biome-fog",
    "sources": "https://github.com/steveplays28/biome-fog",
    "issues": "https://github.com/steveplays28/biome-fog/issues"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/KbWxgGg"
      }
    },
    "mc-publish": {
      "loaders": [
        "fabric",
        "quilt"
      ],
      "curseforge": 855301,
      "modrinth": "kN6Djea2"
    }
  },
  "license": "LGPL-3.0",
  "icon": "assets/biome-fog/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "io.github.steveplays28.biomefog.client.BiomeFogClient"
    ]
  },
  "mixins": [
    "biome-fog.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  },
  "conflicts": {
    "clear-skies": "*"
  },
  "suggests": {
    "sodium": "*"
  },
  "accessWidener": "biome-fog.accesswidener"
}
